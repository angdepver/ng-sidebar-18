{
  "name": "ng-sidebar",
  "version": "9.4.4",
  "description": "Angular sidebar component.",
  "repository": {
    "type": "git",
    "url": "https://github.com/angdepver/ng-sidebar-18.git"
  },
  "homepage": "http://echeung.me/ng-sidebar/",
  "bugs": {
    "url": "https://github.com/angdepver/ng-sidebar-18/issues"
  },
  "files": [
    "lib_commonjs/*",
    "lib_esmodule/*",
    "README.md",
    "LICENSE"
  ],
  "author": "Eugene Cheung",
  "license": "MIT",
  "keywords": [
    "angular",
    "ng",
    "sidebar",
    "component"
  ],
  "main": "./lib_commonjs/index.js",
  "module": "./lib_esmodule/index.js",
  "jsnext:main": "./lib_esmodule/index.js",
  "typings": "./lib_commonjs/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "build:commonjs": "rimraf lib_commonjs && ngc -p tsconfig.json",
    "build:esmodule": "rimraf lib_esmodule && ngc -p tsconfig.module.json",
    "build": "npm run lint && npm run build:commonjs && npm run build:esmodule",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@angular/common": ">=19.1.3",
    "@angular/core": ">=19.1.3"
  },
  "devDependencies": {
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/compiler-cli": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/platform-browser": "^19.1.3",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "tslint": "^5.20.1",
    "typescript": "^5.7.3",
    "zone.js": "^0.15.0"
  }
}
